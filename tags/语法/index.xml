      &lt;tr&gt;
          &lt;th style=&#34;text-align: center&#34;&gt;break&amp;ndash;&amp;gt;broke&lt;/th&gt;
          &lt;th style=&#34;text-align: center&#34;&gt;strike&amp;ndash;&amp;gt;stroke&lt;/th&gt;
          &lt;th style=&#34;text-align: center&#34;&gt;freeze&amp;ndash;&amp;gt;froze&lt;/th&gt;
      &lt;/tr&gt;
  &lt;/thead&gt;
  &lt;tbody&gt;
  &lt;/tbody&gt;
&lt;/table&gt;
