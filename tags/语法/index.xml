      &lt;tr&gt;
          &lt;th style=&#34;text-align: center&#34;&gt;hit&amp;ndash;&amp;gt;hit&lt;/th&gt;
          &lt;th style=&#34;text-align: center&#34;&gt;put&amp;ndash;&amp;gt;put&lt;/th&gt;
          &lt;th style=&#34;text-align: center&#34;&gt;set&amp;ndash;&amp;gt;set&lt;/th&gt;
      &lt;/tr&gt;
